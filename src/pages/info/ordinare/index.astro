---
import BaseLayout from '~/layouts/BaseLayout.astro';
import InfoSection from '~/components/InfoSection.astro';
import { withFallbackSeo } from '~/lib/seo';

export const prerender = true;

const seo = withFallbackSeo([], {
  title: 'Ordinare | Multimage',
  description: 'Stiamo documentando i canali per ordinare le pubblicazioni.',
});
---

<BaseLayout seo={seo}>
  <InfoSection
    title="Ordinare"
    paragraphs={['Stiamo documentando i canali per ordinare le pubblicazioni.']}
    chip="Aggiornamento in arrivo"
  />
</BaseLayout>

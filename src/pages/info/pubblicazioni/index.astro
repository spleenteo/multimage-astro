---
import BaseLayout from '~/layouts/BaseLayout.astro';
import InfoSection from '~/components/InfoSection.astro';
import { withFallbackSeo } from '~/lib/seo';

export const prerender = true;

const seo = withFallbackSeo([], {
  title: 'Pubblicazioni | Multimage',
  description: 'Raccoglieremo gli speciali e gli approfondimenti editoriali.',
});
---

<BaseLayout seo={seo}>
  <InfoSection
    title="Pubblicazioni"
    paragraphs={["Raccoglieremo gli speciali e gli approfondimenti editoriali."]}
    chip="Aggiornamento in arrivo"
  />
</BaseLayout>

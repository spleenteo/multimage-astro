---
import BaseLayout from '~/layouts/BaseLayout.astro';
import InfoSection from '~/components/InfoSection.astro';
import { withFallbackSeo } from '~/lib/seo';

export const prerender = true;

const seo = withFallbackSeo([], {
  title: 'Eirenefest | Multimage',
  description: 'Qui parleremo dell’evento Eirenefest e delle iniziative correlate.',
});
---

<BaseLayout seo={seo}>
  <InfoSection
    title="Eirenefest"
    paragraphs={['Qui parleremo dell’evento Eirenefest e delle iniziative correlate.']}
    chip="Aggiornamento in arrivo"
  />
</BaseLayout>

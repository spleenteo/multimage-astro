---
import BaseLayout from '~/layouts/BaseLayout.astro';
import InfoSection from '~/components/InfoSection.astro';
import { withFallbackSeo } from '~/lib/seo';

export const prerender = true;

const seo = withFallbackSeo([], {
  title: 'Sostieni Multimage | Multimage',
  description: 'Presto indicheremo tutte le modalità di sostegno e donazione.',
});
---

<BaseLayout seo={seo}>
  <InfoSection
    title="Sostieni Multimage"
    paragraphs={['Presto indicheremo tutte le modalità di sostegno e donazione.']}
    chip="Aggiornamento in arrivo"
  />
</BaseLayout>

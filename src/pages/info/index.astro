---
import BaseLayout from '~/layouts/BaseLayout.astro';
import InfoSection from '~/components/InfoSection.astro';
import { withFallbackSeo } from '~/lib/seo';

export const prerender = true;

const seo = withFallbackSeo([], {
  title: 'Info | Multimage',
  description:
    'Qui centralizzeremo le pagine istituzionali attualmente presenti nella copia statica del sito Multimage.',
});
---

<BaseLayout seo={seo}>
  <InfoSection
    title="Info"
    paragraphs={[
      'Qui centralizzeremo le pagine istituzionali (chi siamo, contatti, carta etica) attualmente presenti nella copia statica. I contenuti verranno portati in formato Markdown o CMS.',
    ]}
    chip="Coming soon"
  />
</BaseLayout>

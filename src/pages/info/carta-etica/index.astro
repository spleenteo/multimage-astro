---
import BaseLayout from '~/layouts/BaseLayout.astro';
import InfoSection from '~/components/InfoSection.astro';
import { withFallbackSeo } from '~/lib/seo';

export const prerender = true;

const seo = withFallbackSeo([], {
  title: 'Carta Etica | Multimage',
  description: 'La carta etica dell’associazione sarà disponibile presto in questa sezione.',
});
---

<BaseLayout seo={seo}>
  <InfoSection
    title="Carta Etica"
    paragraphs={["La carta etica dell’associazione sarà disponibile presto in questa sezione."]}
    chip="Aggiornamento in arrivo"
  />
</BaseLayout>
